CREATE TABLE  "REGISTERUSER" 
   (	"OPNO" NUMBER(10,0) NOT NULL ENABLE, 
	"NAME" VARCHAR2(20) NOT NULL ENABLE, 
	"GENDER" VARCHAR2(10) NOT NULL ENABLE, 
	"TYPE" VARCHAR2(10) NOT NULL ENABLE, 
	"BENIFICIARYONE" VARCHAR2(10), 
	"BENIFICIARYTWO" VARCHAR2(20), 
	"ADDRESS" VARCHAR2(30), 
	"AGE" NUMBER(10,0), 
	 CONSTRAINT "REGISTERUSER_PK" PRIMARY KEY ("OPNO") ENABLE
   ) ;CREATE TABLE  "IN_PATIENT_EMPLOYEE" 
   (	"OPNO" NUMBER(10,0) NOT NULL ENABLE, 
	"NAME" VARCHAR2(100) NOT NULL ENABLE, 
	"AGE" NUMBER(30,0) NOT NULL ENABLE, 
	"SEX" VARCHAR2(30) NOT NULL ENABLE, 
	"OCCUPATION" VARCHAR2(100) NOT NULL ENABLE, 
	"DATE_OF_ADMISSION" VARCHAR2(100) NOT NULL ENABLE, 
	"DATE_OF_DISCHARGE" VARCHAR2(100) NOT NULL ENABLE, 
	"DIAGNOSIS" VARCHAR2(100) NOT NULL ENABLE, 
	"PRESENT_COMPLAINT" VARCHAR2(100) NOT NULL ENABLE, 
	"PATIENT_HISTORY" VARCHAR2(100) NOT NULL ENABLE, 
	"DRUG_ALLERY" VARCHAR2(100) NOT NULL ENABLE, 
	"BED_CHARGE" NUMBER(30,0) NOT NULL ENABLE, 
	"XRAY_CHARGE" NUMBER(30,0) NOT NULL ENABLE, 
	"BILLING" NUMBER(30,0) NOT NULL ENABLE
   ) ;CREATE TABLE  "IN_PATIENT_STUDENT" 
   (	"OPNO" NUMBER(10,0) NOT NULL ENABLE, 
	"NAME" VARCHAR2(30) NOT NULL ENABLE, 
	"AGE" NUMBER(10,0) NOT NULL ENABLE, 
	"SEX" VARCHAR2(100) NOT NULL ENABLE, 
	"OCCUPATION" VARCHAR2(30) NOT NULL ENABLE, 
	"DATE_OF_ADMISSION" VARCHAR2(30) NOT NULL ENABLE, 
	"DATE_OF_DISCHARGE" VARCHAR2(30) NOT NULL ENABLE, 
	"DIAGONISES" VARCHAR2(30) NOT NULL ENABLE, 
	"PRESENT_COMPLAINT" VARCHAR2(10) NOT NULL ENABLE, 
	"PATIENT_HISTORY" VARCHAR2(30) NOT NULL ENABLE, 
	"DRUG_ALLERGY" VARCHAR2(30) NOT NULL ENABLE
   ) ;CREATE TABLE  "LOGIN" 
   (	"ID" VARCHAR2(20), 
	"PASSWORD" VARCHAR2(20)
   ) ;CREATE TABLE  "MAIN_STORE" 
   (	"DRUG_NAME" VARCHAR2(50) NOT NULL ENABLE, 
	"IN_STORE" NUMBER(20,0) NOT NULL ENABLE, 
	"EXPIRY_DATE_1" VARCHAR2(50) NOT NULL ENABLE, 
	"EXPIRY_DATE_2" VARCHAR2(50) NOT NULL ENABLE, 
	 CONSTRAINT "MAIN_STORE_PK" PRIMARY KEY ("DRUG_NAME", "IN_STORE") ENABLE
   ) ;CREATE TABLE  "PHARMACY_STORE" 
   (	"DRUG_NAME" VARCHAR2(50) NOT NULL ENABLE, 
	"IN_STOCK" NUMBER(20,0) NOT NULL ENABLE, 
	"EXPIRY_DATE_1" VARCHAR2(50) NOT NULL ENABLE, 
	"EXPIRY_DATE_2" VARCHAR2(50) NOT NULL ENABLE, 
	 CONSTRAINT "PHARMACY_STORE_PK" PRIMARY KEY ("DRUG_NAME") ENABLE
   ) ;CREATE TABLE  "NURSE_ENTRY" 
   (	"OP_NO" NUMBER(10,0) NOT NULL ENABLE, 
	"DRUG_NAME" VARCHAR2(100) NOT NULL ENABLE, 
	"MED_GIVEN_DATE" VARCHAR2(100) NOT NULL ENABLE, 
	"TIME" VARCHAR2(100) NOT NULL ENABLE, 
	"QUANTITY" NUMBER(10,0) NOT NULL ENABLE
   ) ;CREATE TABLE  "OPERATOR_ENTRY" 
   (	"OP_NO" NUMBER(10,0) NOT NULL ENABLE, 
	"Drug_Name" VARCHAR2(100) NOT NULL ENABLE, 
	"DRUG_Date" VARCHAR2(100) NOT NULL ENABLE, 
	"NO_OF_DRUGS" NUMBER(10,0) NOT NULL ENABLE
   ) ;CREATE TABLE  "OUT_PATIENT" 
   (	"OPNO" NUMBER(30,0) NOT NULL ENABLE, 
	"NAME" VARCHAR2(100) NOT NULL ENABLE, 
	"AGE" NUMBER(30,0) NOT NULL ENABLE, 
	"TYPE" VARCHAR2(100) NOT NULL ENABLE, 
	"DATE_OF_ADMISSION" VARCHAR2(100) NOT NULL ENABLE, 
	"SEX" VARCHAR2(100) NOT NULL ENABLE, 
	"DIAGNOSIS" VARCHAR2(30) NOT NULL ENABLE, 
	"TREATMENT" VARCHAR2(30) NOT NULL ENABLE
   ) ;ALTER TABLE  "IN_PATIENT_EMPLOYEE" ADD CONSTRAINT "IN_PATIENT_EMPLOYEE_FK" FOREIGN KEY ("OPNO")
	  REFERENCES  "REGISTERUSER" ("OPNO") ENABLE;ALTER TABLE  "IN_PATIENT_STUDENT" ADD CONSTRAINT "IN_PATIENT_STUDENT_FK" FOREIGN KEY ("OPNO")
	  REFERENCES  "REGISTERUSER" ("OPNO") ENABLE;ALTER TABLE  "NURSE_ENTRY" ADD CONSTRAINT "NURSE_ENTRY_FK" FOREIGN KEY ("DRUG_NAME")
	  REFERENCES  "PHARMACY_STORE" ("DRUG_NAME") ENABLE;ALTER TABLE  "OPERATOR_ENTRY" ADD CONSTRAINT "OPERATOR_ENTRY_FK" FOREIGN KEY ("Drug_Name")
	  REFERENCES  "PHARMACY_STORE" ("DRUG_NAME") ENABLE;ALTER TABLE  "OUT_PATIENT" ADD CONSTRAINT "OUT_PATIENT_FK" FOREIGN KEY ("OPNO")
	  REFERENCES  "REGISTERUSER" ("OPNO") ENABLE;
